    "browser-sync": "^2.26.5",
    "browser-sync-webpack-plugin": "2.0.1",
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.15",
    "mix-html-builder": "git://github.com/mikemartin/mix-html-builder.git",
    "posthtml": "^0.11.4",
    "posthtml-include": "^1.2.0",
    "posthtml-loader": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  }
